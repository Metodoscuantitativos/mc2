pacman::p_load(tidyverse, readxl, haven, openxlsx)

MIS<-read_xlsx("BBDD MIS ALTO MAIPO.xlsx", na = c("99\tNS-NR", "Respuesta"))

prop.table(table(MIS$`3.	La vivienda que usted ocupa es (ENC: LEER ALTERNATIVAS):`))*100


MIS$`4.	¿Cuál es el valor que paga mensualmente?` <- as.numeric(MIS$`4.	¿Cuál es el valor que paga mensualmente?`)


`4.	¿Cuál es el valor que paga mensualmente?`<-as.numeric(MIS$`4.	¿Cuál es el valor que paga mensualmente?`)


MIS<-MIS %>% mutate(precio_rec= case_when(`4.	¿Cuál es el valor que paga mensualmente?` <=100000 ~ "menor a 100.000",
                                        `4.	¿Cuál es el valor que paga mensualmente?` >100000 & `4.	¿Cuál es el valor que paga mensualmente?`<= 200000 ~ "entre 100 mil y 200 mil",
                                        `4.	¿Cuál es el valor que paga mensualmente?` >200000 & `4.	¿Cuál es el valor que paga mensualmente?`<= 300000 ~ "entre 200 mil y 300 mil",
                                        `4.	¿Cuál es el valor que paga mensualmente?` >300000 & `4.	¿Cuál es el valor que paga mensualmente?`<= 400000 ~ "entre 300 mil y 400 mil",
                                        `4.	¿Cuál es el valor que paga mensualmente?` >400000 & `4.	¿Cuál es el valor que paga mensualmente?`<= 500000 ~ "entre 400 mil y 500 mil",
                                        `4.	¿Cuál es el valor que paga mensualmente?` >500000 ~ "más de 500 mil"))

prop.table(table(MIS$`4.	¿Cuál es el valor que paga mensualmente?`))*100

prop.table(table(MIS$precio_rec))*100
