t1<- t1[-1, -1]
t2<- t2[-1, -1]
t3<- t3[-1, -1]
t4<- t4[-1, -1]
t5<- t5[-1, -1]

t1<- as.data.frame(t1)
t2<- as.data.frame(t2)
t3<- as.data.frame(t3)
t4<- as.data.frame(t4)
t5<- as.data.frame(t5)





t1<-t1 %>% mutate(Var1="tiempo de espera en atención a urgencia")
t2<-t2 %>% mutate(Var1="tiempo de espera para interconsulta")
t3<-t3 %>% mutate(Var1="acceso a exámenes")
t4<-t4 %>% mutate(Var1="acceso a hospitalización")
t5<-t5 %>% mutate(Var1="acceso a cirugía")


t<-rbind(t1,t2,t3,t4,t5)


ggplot(t, aes(fill=var1, y=Freq, x=Var2)) + 
  geom_bar(position="dodge", stat="identity")


ggplot(t, aes(fill=Var2, y=Freq, x=Var1)) + 
  geom_bar(position="fill", stat="identity")


# Agregar colores y etiquetas en color blanco al gráfico
ggplot(t, aes(fill=Var2, y=Freq, x=Var1)) + 
  geom_bar(position="fill", stat="identity") +
  scale_fill_manual(values = c("#7F7F7F", "#254061", "#C00000"))  


library(stringr)

# Crear el gráfico de barras con nombres de variables en eje x con salto de línea
g<-ggplot(t, aes(fill=Var2, y=Freq, x=str_wrap(Var1, width = 10))) + 
  geom_bar(position="fill", stat="identity") +
  scale_fill_manual(values = c("#7F7F7F", "#254061", "#C00000")) +
  labs(x = "", y = "") +
  theme(axis.text.x = element_text(angle = 0, hjust = 1))+
  theme_minimal()+
  theme(legend.position = "bottom", legend.justification = "center", text = element_text(family = "Century Gothic", size = 10))



ggsave("graficos/g.jpg", plot = g, dpi = 200)


