#Cruce lugar de traslado con medio de transporte

table(MIS$medio_transporte_1,MIS$lugar_traslado_1)

write.xlsx(t, "graficos/cruce.xlsx")

unique(MIS$medio_transporte_4)

df<-MIS %>% select(lugar_traslado_1,lugar_traslado_2,lugar_traslado_3,lugar_traslado_4, )

MIS<- MIS %>% mutate(medio_transporte_2=case_when(medio_transporte_2=="2 - Medio de Transporte"~NA,
                                                  medio_transporte_2=="15" ~ NA,
                                                  TRUE ~medio_transporte_2))

MIS<- MIS %>% mutate(medio_transporte_4=case_when(medio_transporte_4=="no aplica"~NA,
                                                  medio_transporte_4=="Bicicleta" ~ NA,
                                                  TRUE ~medio_transporte_4))


MIS<- MIS %>% mutate(lugar_traslado_1=case_when(lugar_traslado_1=="1. En la localidad donde vive"~"1",
                                                lugar_traslado_1=="2. Hacia otra localidad" ~"2",
                                                lugar_traslado_1=="3. Al centro de San José de Maipo "~"3",
                                                lugar_traslado_1=="4. Hacia otra comuna" ~"4",
                                                lugar_traslado_1=="5. Hacia otra región"~"5",
                                                TRUE ~ lugar_traslado_1))

MIS$lugar_traslado_1<- as.factor(MIS$lugar_traslado_1)

MIS<- MIS %>% mutate(lugar_traslado_2=case_when(lugar_traslado_2=="1. En la localidad donde vive"~"1",
                                                lugar_traslado_2=="2. Hacia otra localidad" ~"2",
                                                lugar_traslado_2=="3. Al centro de San José de Maipo "~"3",
                                                lugar_traslado_2=="4. Hacia otra comuna" ~"4",
                                                lugar_traslado_2=="5. Hacia otra región"~"5",
                                                TRUE ~ lugar_traslado_2))
MIS$lugar_traslado_2<- as.factor(MIS$lugar_traslado_2)

MIS<- MIS %>% mutate(lugar_traslado_3=case_when(lugar_traslado_3=="1. En la localidad donde vive"~"1",
                                                lugar_traslado_3=="2. Hacia otra localidad" ~"2",
                                                lugar_traslado_3=="3. Al centro de San José de Maipo "~"3",
                                                lugar_traslado_3=="4. Hacia otra comuna" ~"4",
                                                lugar_traslado_3=="5. Hacia otra región"~"5",
                                                TRUE ~ lugar_traslado_3))
MIS$lugar_traslado_3<- as.numeric(MIS$lugar_traslado_3)

MIS<- MIS %>% mutate(lugar_traslado_4=case_when(lugar_traslado_4=="1. En la localidad donde vive"~"1",
                                                lugar_traslado_4=="2. Hacia otra localidad" ~"2",
                                                lugar_traslado_4=="3. Al centro de San José de Maipo "~"3",
                                                lugar_traslado_4=="4. Hacia otra comuna" ~"4",
                                                lugar_traslado_4=="5. Hacia otra región"~"5",
                                                TRUE ~ lugar_traslado_4))
MIS$lugar_traslado_4<- as.numeric(MIS$lugar_traslado_4)




  MIS<- MIS %>% mutate(lugar_traslado=lugar_traslado_1+lugar_traslado_2)
