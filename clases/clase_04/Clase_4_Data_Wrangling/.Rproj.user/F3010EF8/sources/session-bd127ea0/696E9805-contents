#DETENIDOS
detenidos <- read_excel("tramitacionfinal669953.xlsx", skip = 4, sheet = 3) 

#2022 primer semestre
detenidos_2023<- detenidos %>% filter(`FECHA DELITO` >=45047 & `FECHA DELITO`<=45169)

detenidos_2023 %>% group_by(DELITO) %>% summarise(total=sum(`TOTAL DETENIDOS`))
  
detenidos_t1_2022 %>% group_by(DELITO) %>% summarise(total=sum(`TOTAL DETENIDOS`))
#2022 segundo semestre
detenidos_t2_2022<- detenidos %>% filter(`FECHA DELITO` >=44758 & `FECHA DELITO`<=44914)
detenidos_t2_2022 %>% group_by(DELITO) %>% summarise(total=sum(`TOTAL DETENIDOS`))
#2023 primer semestre
detenidos_t1_2023<- detenidos %>% filter(`FECHA DELITO` >=44940 & `FECHA DELITO`<=45101)
detenidos_t1_2023 %>% group_by(DELITO) %>% summarise(total=sum(`TOTAL DETENIDOS`))
#2023 segundo semestre
detenidos_t2_2023<- detenidos %>% filter(`FECHA DELITO` >=45130 & `FECHA DELITO`<=45255)
detenidos_t2_2023 %>% group_by(DELITO) %>% summarise(total=sum(`TOTAL DETENIDOS`))





#detenidos<-detenidos[1:84,]
detenidos_trans<-detenidos %>% filter(`FECHA DELITO`>= 44673& `FECHA DELITO` <= 45074)

t<-detenidos_trans %>% select(DELITO,`TOTAL DETENIDOS`) %>%  group_by(DELITO) %>% summarise(total= sum(`TOTAL DETENIDOS`))

sum(detenidos_trans$`TOTAL DETENIDOS`)

write.csv(t, file = "t.csv") 
