#Delitos

delitos <- read_excel("tramitacionfinal669953.xlsx", skip =4) 

#primer semestre 2022                                     45047      45169
delitos_2023<- delitos %>% filter(`FECHA DELITO` >=45047 & `FECHA DELITO`<=45169)
table(delitos_2023$DELITO)



delitos_2023 %>% group_by (DELITO) %>% summarise(total=sum(`TOTAL CASOS`))


#segundo semestre 2022
delitos_t2_2022<- delitos %>% filter(`FECHA DELITO` >=44758 & `FECHA DELITO`<=44914)
table(delitos_t2_2022$DELITO)

delitos_t2_2022 %>% summarise(total=sum(`TOTAL CASOS`))

#primer semestre 2023
delitos_t1_2023<- delitos %>% filter(`FECHA DELITO` >=44940 & `FECHA DELITO`<=45101)
table(delitos_t1_2023$DELITO)

delitos_t1_2023 %>% summarise(total=sum(`TOTAL CASOS`))

#segundo semestre 2023
delitos_t2_2023<- delitos %>% filter(`FECHA DELITO` >=45130 & `FECHA DELITO`<=45255)
table(delitos_t2_2023$DELITO)

delitos_t2_2023 %>% summarise(total=sum(`TOTAL CASOS`))

