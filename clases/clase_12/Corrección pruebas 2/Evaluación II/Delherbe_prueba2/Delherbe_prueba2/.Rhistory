# 01. Cargar datos--------------------------------------------------------
base <- read.xlsx("base_antropologia_limpia.xlsx")
nrow(base) # 147 cantidad de casos/filas
ncol(base) # 60 cantidad de variables/columnas
names(base)
base <- base %>% dplyr::rename(tiempo_libre_semanal= to_01,
actividades_ocio= to_04,
participacion_colectivos= to_05,
importancia_ocio= to_06)
names(base)
base <-base %>% rename(lugar_musica=cm_05)
unique(base$lugar_musica)
respuestas <- strsplit(base$lugar_musica, ",")
respuestas <- unlist(respuestas)
freq(respuestas, prop=TRUE, order = "freq", report.nas = FALSE) %>%
tb()
respuestas <- gsub("En el transporte público", "En el transporte público", respuestas)
respuestas <-gsub("En otros lugares", "En otros lugares", respuestas)
freq(respuestas, prop=TRUE, order = "freq", report.nas = FALSE) %>%
tb()
olivier_delherbe <-  freq(respuestas, prop=TRUE, order = "freq", report.nas = FALSE) %>%
tb()
